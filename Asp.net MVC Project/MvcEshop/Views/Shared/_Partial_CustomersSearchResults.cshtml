@model List<MvcEshop.Models.Customer>

@foreach (MvcEshop.Models.Customer customer in Model)
{
    <div class="container">
        <div class="col-md-9 alert alert-success" style="font-weight:bold;">
            <div class=" col-md-offset-1 col-md-8">@customer.Name</div>
            <div class="col-md-1">
                <a href="#" class="JsonAction">
                    <img id="@customer.CustomerId" src="~/Content/Files/Icons/mail_send.png" />
                </a>

            </div>
        </div>
    </div>
}


